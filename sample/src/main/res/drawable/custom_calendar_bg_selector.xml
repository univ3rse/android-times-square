<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:app="http://schemas.android.com/apk/res-auto">
    <item app:tsquare_state_range_first_overall="true" android:drawable="@drawable/bg_circle_today">
    </item>
    <item app:tsquare_state_range_first="true" android:drawable="@drawable/ic_bg_start_range">
    </item>
    <item app:tsquare_state_range_last="true" android:drawable="@drawable/ic_bg_end_range">
    </item>
    <item app:tsquare_state_range_middle="true">
        <color android:color="@color/custom_background_today"/>
    </item>
    <item android:state_selected="true">
        <color android:color="@color/custom_background_today"/>
    </item>
    <item android:state_pressed="true">
        <color android:color="@color/custom_background_white"/>
    </item>
    <item app:tsquare_state_current_month="false">
        <color android:color="@color/custom_background_white"/>
    </item>
    <item app:tsquare_state_highlighted="true">
        <color android:color="@color/custom_background_white"/>
    </item>
    <item app:tsquare_state_today="true">
        <color android:color="@color/custom_background_white"/>
    </item>
    <item>
        <color android:color="@color/custom_background_white"/>
    </item>
</selector>
